@page
@model Ganaderia.App.Presentacion.Pages.VaccinesModel
@{
}
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="~/css/Predio.css"></link>
</head>
<body>
    <table class="table table-striped">
        <h2>Vacunas</h2>
        <thead>
            <tr>
                <th>Id</th>
                <th>Nombre</th>
                <th>Lote</th>
                <th>Laboratorio</th>
                <th>Opciones</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var vaccine in Model.Vaccines)
            {
                <tr>
                <td>@vaccine.Id</td>
                <td>@vaccine.Name</td>
                <td>@vaccine.Lot</td>
                <td>@vaccine.Lab</td>
                <td>
                    <button type="submit">Eliminar</button>
                </td>
                </tr>
            }
        </tbody>
    </table>
</body>
</html>